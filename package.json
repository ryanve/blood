{
  "name": "blood",
  "description": "Object utilities",
  "version": "0.9.0",
  "homepage": "https://github.com/ryanve/blood",
  "license": "MIT",
  "author": "Ryan Van Etten",
  "keywords": [
    "util",
    "utility",
    "object",
    "objects",
    "functional",
    "inheritance",
    "ender",
    "javascript",
    "es5"
  ],
  "main": "blood.js",
  "ender": "noop",
  "scripts": {
    "lint": "eslint . --ext .js",
    "preversion": "npm test",
    "pretest": "npm run lint",
    "test": "node test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ryanve/blood.git"
  },
  "dependencies": {},
  "devDependencies": {
    "aok": "^1.9.0",
    "eslint": "^3.16.1"
  },
  "jshintConfig": {
    "asi": false,
    "boss": true,
    "browser": true,
    "debug": true,
    "devel": true,
    "eqnull": true,
    "evil": true,
    "expr": true,
    "lastsemic": true,
    "latedef": "nofunc",
    "laxcomma": true,
    "maxcomplexity": 15,
    "maxerr": 10,
    "maxlen": 115,
    "maxparams": 4,
    "node": true,
    "sub": true,
    "supernew": true,
    "trailing": true,
    "undef": true,
    "unused": true,
    "worker": true
  }
}
